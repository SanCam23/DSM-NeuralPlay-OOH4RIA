<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="NeuralPlayGen.Infraestructure"
		                   namespace="NeuralPlayGen.Infraestructure.EN.NeuralPlay">
		    
		    
			
				<class name="PerfilJuegoNH" table="PerfilJuego"> 
				  	
						<id name="Id" column="idPerfilJuego" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="FechaAccion" type="date"> 
						<column name="fechaAccion"  not-null="true"/>
					</property>
			
			
			
			
		  			
						<many-to-one name="Perfil" class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.PerfilNH" >	
								<column name="FK_idPerfil_idPerfil"/> 
						</many-to-one>
											 					
			
		  			
						<many-to-one name="Juego" class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.JuegoNH" >	
								<column name="FK_idJuego_idJuego"/> 
						</many-to-one>
											 					
					
					
					
					
		
		 </class>	
		  
		  <query name="PerfilJuegoNHReadAllHQL">from PerfilJuegoNH</query>
		</hibernate-mapping>
	