<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="NeuralPlayGen.Infraestructure"
		                   namespace="NeuralPlayGen.Infraestructure.EN.NeuralPlay">
		    
		    
			
				<class name="VotoTorneoNH" table="VotoTorneo"> 
				  	
						<id name="Id" column="idVotoTorneo" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Valor" type="boolean"> 
						<column name="valor"  not-null="true"/>
					</property>
					<property name="FechaVoto" type="date"> 
						<column name="fechaVoto"  not-null="true"/>
					</property>
			
			
			
			
			
		  			
						<many-to-one name="PropuestaTorneo" class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.PropuestaTorneoNH" >	
								<column name="FK_idPropuestaTorneo_idPropuestaTorneo"/> 
						</many-to-one>
											 					
			
		  			
						<many-to-one name="Usuario" class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.UsuarioNH" >	
								<column name="FK_idUsuario_idUsuario"/> 
						</many-to-one>
											 					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="VotoTorneoNHReadAllHQL">from VotoTorneoNH</query>
		</hibernate-mapping>
	