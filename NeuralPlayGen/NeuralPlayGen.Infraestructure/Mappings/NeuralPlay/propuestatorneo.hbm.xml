<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="NeuralPlayGen.Infraestructure"
		                   namespace="NeuralPlayGen.Infraestructure.EN.NeuralPlay">
		    
		    
			
				<class name="PropuestaTorneoNH" table="PropuestaTorneo"> 
				  	
						<id name="Id" column="idPropuestaTorneo" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="FechaPropuesta" type="date"> 
						<column name="fechaPropuesta"  not-null="true"/>
					</property>
					<property name="Estado" > 
						<column name="estado"  not-null="true"/>
					</property>
					<property name="Nombre" type="string"> 
						<column name="nombre"  not-null="true"/>
					</property>
			
			
			
			
			
		  			
						<many-to-one name="Torneo" class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.TorneoNH" >	
								<column name="FK_idTorneo_idTorneo"/> 
						</many-to-one>
											 					
			
		  			
						<many-to-one name="Equipo" class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.EquipoNH" >	
								<column name="FK_idEquipo_idEquipo"/> 
						</many-to-one>
											 					
			
		  			
						<bag name="VotoTorneo" cascade="delete">
							<key>
							
								
								<column name="FK_idPropuestaTorneo_idPropuestaTorneo"/>
					      	</key>
							<one-to-many class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.VotoTorneoNH"/>
					    </bag>
					
			
		  			
						<bag name="Notificacion" >
							<key>
							
								
								<column name="FK_idPropuestaTorneo_idPropuestaTorneo"/>
					      	</key>
							<one-to-many class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.NotificacionNH"/>
					    </bag>
					
			
		  			
						<many-to-one name="Usuario" class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.UsuarioNH" >	
								<column name="FK_idUsuario_idUsuario"/> 
						</many-to-one>
											 					
			
			
		  			
						<bag name="Comunidad" >
							<key>
							
								
								<column name="FK_idPropuestaTorneo_idPropuestaTorneo"/>
					      	</key>
							<one-to-many class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.ComunidadNH"/>
					    </bag>
					
					
					
					
					
					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="PropuestaTorneoNHReadAllHQL">from PropuestaTorneoNH</query>
		</hibernate-mapping>
	