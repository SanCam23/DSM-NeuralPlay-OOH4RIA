<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="NeuralPlayGen.Infraestructure"
		                   namespace="NeuralPlayGen.Infraestructure.EN.NeuralPlay">
		    
		    
			
				<class name="ComunidadNH" table="Comunidad"> 
				  	
						<id name="Id" column="idComunidad" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Nombre" type="string"> 
						<column name="nombre"  not-null="true"/>
					</property>
					<property name="Descripcion" type="string"> 
						<column name="descripcion"  not-null="true"/>
					</property>
					<property name="FechaCreacion" type="date"> 
						<column name="fechaCreacion"  not-null="true"/>
					</property>
			
			
			
			
			
			
		  			
						<bag name="Equipo" cascade="delete">
							<key>
							
								
								<column name="FK_idComunidad_idComunidad"/>
					      	</key>
							<one-to-many class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.EquipoNH"/>
					    </bag>
					
			
		  			
						<bag name="Torneo" >
							<key>
							
								
								<column name="FK_idComunidad_idComunidad"/>
					      	</key>
							<one-to-many class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.TorneoNH"/>
					    </bag>
					
			
		  			
					
							<many-to-one name="Invitacion" class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.InvitacionNH" >
								<column name="FK_idInvitacion_idInvitacion"/> 
        					</many-to-one>
					 
						
			
		  			
						<bag name="MiembroComunidad" >
							<key>
							
								
								<column name="FK_idComunidad_idComunidad"/>
					      	</key>
							<one-to-many class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.MiembroComunidadNH"/>
					    </bag>
					
			
		  			
						<bag name="Publicacion" cascade="delete">
							<key>
							
								
								<column name="FK_idComunidad_idComunidad"/>
					      	</key>
							<one-to-many class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.PublicacionNH"/>
					    </bag>
					
			
		  			
						<bag name="SolicitudIngreso" >
							<key>
							
								
								<column name="FK_idComunidad_idComunidad"/>
					      	</key>
							<one-to-many class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.SolicitudIngresoNH"/>
					    </bag>
					
			
		  			
						<many-to-one name="PropuestaTorneo" class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.PropuestaTorneoNH" >	
								<column name="FK_idPropuestaTorneo_idPropuestaTorneo"/> 
						</many-to-one>
											 					
					
					
					
					
					
					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="ComunidadNHReadAllHQL">from ComunidadNH</query>
		</hibernate-mapping>
	