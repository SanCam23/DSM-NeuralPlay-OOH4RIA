<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="NeuralPlayGen.Infraestructure"
		                   namespace="NeuralPlayGen.Infraestructure.EN.NeuralPlay">
		    
		    
			
				<class name="EquipoNH" table="Equipo"> 
				  	
						<id name="Id" column="idEquipo" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Nombre" type="string"> 
						<column name="nombre"  not-null="true"/>
					</property>
					<property name="Descripcion" type="string"> 
						<column name="descripcion"  not-null="true"/>
					</property>
					<property name="FechaCreacion" type="date"> 
						<column name="fechaCreacion"  not-null="true"/>
					</property>
			
			
			
			
			
			
		  			
						<many-to-one name="Comunidad" class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.ComunidadNH" >	
								<column name="FK_idComunidad_idComunidad"/> 
						</many-to-one>
											 					
			
		  			
						<bag name="MiembroEquipo" cascade="delete">
							<key>
							
								
								<column name="FK_idEquipo_idEquipo"/>
					      	</key>
							<one-to-many class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.MiembroEquipoNH"/>
					    </bag>
					
			
		  			
						<bag name="PropuestaTorneo" >
							<key>
							
								
								<column name="FK_idEquipo_idEquipo"/>
					      	</key>
							<one-to-many class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.PropuestaTorneoNH"/>
					    </bag>
					
			
		  			
						<many-to-one name="ChatEquipo" class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.ChatEquipoNH" cascade="delete">	
								<column name="FK_idChatEquipo_idChatEquipo"/> 
						</many-to-one>
											 					
			
		  			
						<bag name="Invitacion" >
							<key>
							
								
								<column name="FK_idEquipo_idEquipo"/>
					      	</key>
							<one-to-many class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.InvitacionNH"/>
					    </bag>
					
			
		  			
						<bag name="SolicitudIngreso" >
							<key>
							
								
								<column name="FK_idEquipo_idEquipo"/>
					      	</key>
							<one-to-many class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.SolicitudIngresoNH"/>
					    </bag>
					
			
		  			
						<bag name="ParticipacionTorneo" >
							<key>
							
								
								<column name="FK_idEquipo_idEquipo"/>
					      	</key>
							<one-to-many class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.ParticipacionTorneoNH"/>
					    </bag>
					
			
		  			
						<bag name="Juego" table="[juego_equipo]" inverse="true" >
							<key>
								
									<column name="FK_idEquipo_idEquipo"/> 
							</key>
      						<many-to-many class="NeuralPlayGen.Infraestructure.EN.NeuralPlay.JuegoNH">
									<column name="FK_idJuego_idJuego"/> 
      						</many-to-many>
    					</bag>
					
					
					
					
					
					
					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="EquipoNHReadAllHQL">from EquipoNH</query>
		</hibernate-mapping>
	